{
  "name": "react-voice-action-router",
  "version": "1.2.0",
  "description": "A headless voice action router for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "require": "./dist/adapters/index.js",
      "import": "./dist/adapters/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:deploy": "npm run docs:build && gh-pages -d docs/.vitepress/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nouman64-cat/react-voice-action-router.git"
  },
  "keywords": [],
  "author": "Nouman64-cat",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Nouman64-cat/react-voice-action-router/issues"
  },
  "homepage": "https://github.com/Nouman64-cat/react-voice-action-router#readme",
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "gh-pages": "^6.3.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  }
}
